<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence cho mã tài sản -->
        <record id="seq_asset" model="ir.sequence">
            <field name="name">Mã Tài Sản</field>
            <field name="code">asset</field>
            <field name="prefix">TS</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho mã phòng họp -->
        <record id="seq_meeting_room" model="ir.sequence">
            <field name="name">Mã Phòng Họp</field>
            <field name="code">meeting.room</field>
            <field name="prefix">PH</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequence cho mã đặt phòng -->
        <record id="seq_meeting_room_booking" model="ir.sequence">
            <field name="name">Mã Đặt Phòng Họp</field>
            <field name="code">meeting.room.booking</field>
            <field name="prefix">BK</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
